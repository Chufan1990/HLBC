<launch>
	<env name="ROSCONSOLE_CONFIG_FILE" value="$(find hlbc)/conf/custom_rosconsole.conf"/>
	<param name="/use_sim_time" value="true"/>
	<node pkg="hlbc" type="static_path_generator_test" name="static_path_generator" output="screen">
		<param name="filename" value="/autoware.ai/src/autoware/common/hlbc/data/saved_waypoints_CSCp.csv"/>
		<param name="periodic" value="true"/>
		<param name="timestep" value="0.2"/>
	</node>
</launch>
